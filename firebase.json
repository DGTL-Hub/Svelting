{
  "hosting": {
	"site": "digital-land",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "runtime": "nodejs14",
    "rewrites": [
      {
        "source": "**",
        "function": "sveltekit"
      }
    ]
  },
  "functions": {
    "source": "functions"
  },
  "database": {
    "rules": "database.rules.json"
  }
}
